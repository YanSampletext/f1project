	<test id="test_exchange" name="Тест обмена РМУ-4 -> РЛСМ-10-64 #1" file="test_exchange1" proto_src="transparent" proto_dst="transparent" device_src="rlsm1" device_dst="rlsm2" duration="60" length="32" waiting="3000">
	</test>

